[project]
	description = Wikibase Client, Lib, Repo, View and browser tests
[access]
	inheritFrom = mediawiki/extensions
[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group extension-Wikibase
	owner = group wikidata
	push = +force group extension-Wikibase
	pushTag = +force group extension-Wikibase
	create = group extension-Wikibase
	rebase = group extension-Wikibase
	abandon = group extension-Wikibase
	submit = group extension-Wikibase
	editTopicName = group extension-Wikibase
[access "refs/heads/master"]
	exclusiveGroupPermissions = label-Verified submit
	label-Verified = -1..+2 group JenkinsBot
	label-Verified = -1..+2 group l10n-bot
	label-Verified = -1..+2 group wikidata
	label-Verified = -1..+0 group Registered Users
	submit = deny group Registered Users
	submit = group JenkinsBot
	submit = group l10n-bot
	submit = group wikidata
[submit]
	mergeContent = true
[access "refs/tags/*"]
	create = group extension-Wikibase
	push = +force group extension-Wikibase
	owner = group extension-Wikibase
[allowSuperproject "mediawiki/extensions"]
	matching = refs/heads/master:refs/heads/master
