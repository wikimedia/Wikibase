{
	"name": "Wikibase Client",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Wikibase_Client",
	"descriptionmsg": "wikibase-client-desc",
	"license-name": "GPL-2.0+",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"WikibaseLib": "*",
			"Wikibase View": "*"
		}
	},
	"callback": "\\Wikibase\\Client\\Registrar::registerExtension",
	"MessagesDirs": {
		"wikibaseclient": "i18n"
	},
	"ExtensionMessagesFiles": {
		"Wikibaseclientalias": "WikibaseClient.i18n.alias.php",
		"wikibaseclientmagic": "WikibaseClient.i18n.magic.php"
	},
	"TrackingCategories": [
		"unresolved-property-category"
	],
	"RecentChangesFlags": {
		"wikibase-edit": {
			"letter": "wikibase-rc-wikibase-edit-letter",
			"title": "wikibase-rc-wikibase-edit-title",
			"legend": "wikibase-rc-wikibase-edit-legend",
			"grouping": "all"
		}
	},
	"AutoloadClasses": {
		"Wikibase\\Client\\Registrar": "Registrar.php",
		"Wikibase\\ClientHooks": "WikibaseClient.hooks.php",
		"Wikibase\\PopulateEntityUsage": "maintenance/populateEntityUsage.php",
		"Wikibase\\PopulateInterwiki": "maintenance/populateInterwiki.php",
		"Wikibase\\Test\\MockClientStore": "tests/phpunit/MockClientStore.php",
		"Wikibase\\UpdateSubscriptions": "maintenance/updateSubscriptions.php"
	},
	"AutoloadNamespaces": {
		"Wikibase\\Client\\": "includes/",
		"Wikibase\\Client\\Tests\\": "tests/phpunit/includes/"
	},
	"manifest_version": 2
}
