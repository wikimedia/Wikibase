    "eslint-plugin-vue": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^10.1.0",
    "stylelint-config-wikimedia": "^0.6.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.8.0",
    "ts-jest": "^23.10.5",
