{
    "name": "wikibase/wikibase",
    "type": "mediawiki-extension",
    "description": "Structured data repository for MediaWiki",
    "keywords": [
		"wikibase",
		"wikidata",
		"wikibaselib",
		"wikibaseclient",
		"wikibaserepo"
	],
    "homepage": "https://www.mediawiki.org/wiki/Wikibase",
    "license": "GNU GPL v2+",
    "authors": [
        {
            "name": "The Wikidata team"
        }
    ],
	"support": {
		"issues": "https://bugzilla.wikimedia.org/",
		"irc": "irc://irc.freenode.net/wikidata",
		"forum": "https://www.mediawiki.org/wiki/Talk:Wikibase",
		"wiki": "https://www.mediawiki.org/wiki/Wikibase"
	},
    "require": {
        "php": ">=5.3.2",
		"data-values/data-values": "~0.1",
		"data-values/common": "~0.1",
		"data-values/geo": "~0.1",
		"data-values/number": "~0.2",
		"data-values/time": "~0.1",
		"data-values/validators": "~0.1",
		"data-values/data-types": "~0.1",
		"data-values/serialization": "~0.1",
		"data-values/javascript": "0.*,>=0.1.1.1",
		"data-values/value-view": "0.*,>=0.1.1",
		"wikibase/data-model": "0.6.*",
        "diff/diff": ">=0.9"
    },
	"autoload": {
		"files" : [
			"Wikibase.composer.php"
		],
		"classmap": [
			"client/includes/",
			"client/WikibaseClient.hooks.php",
			"client/tests/phpunit/MockPageUpdater.php",

			"lib/includes/",
			"lib/WikibaseLib.hooks.php",
			"lib/tests/phpunit/",

			"repo/includes/",
			"repo/maintenance/",
			"repo/tests/phpunit/",
			"repo/Wikibase.hooks.php"
		]
	}
}
