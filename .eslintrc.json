{
	"extends": "wikimedia",
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"mediaWiki": false,
		"dataTypes": false,
		"dataValues": false,
		"OO": false,
		"require": "false",
		"util": false,
		"valueFormatters": false,
		"valueParsers": false,
		"wikibase": false
	},
	"rules": {
		"indent": "off",
		"new-cap": "off",
		"no-underscore-dangle": "off",
		"no-unused-vars": "off",
		"no-useless-concat": "off",
		"one-var": "off",
		"valid-jsdoc": "off",
		"vars-on-top": "off"
	}
}
